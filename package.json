{
  "name": "@nuxt/a11y",
  "packageManager": "pnpm@10.24.0",
  "version": "0.0.1",
  "description": "Nuxt module to provide accessibility hinting and utilities.",
  "repository": "nuxt/a11y",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "client:build": "nuxt generate client",
    "client:dev": "nuxt dev client --port 3030",
    "dev": "npm run play:dev",
    "prepack": "pnpm run build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare client && nuxt prepare playground",
    "play:dev": "nuxt dev playground",
    "play:prod": "npm run build && nuxt dev playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:types": "vue-tsc --noEmit",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^3.1.1",
    "@nuxt/kit": "^4.2.1",
    "axe-core": "^4.11.0",
    "sirv": "^3.0.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "1.11.0",
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.2.1",
    "@nuxt/test-utils": "3.20.1",
    "@vitest/coverage-v8": "4.0.15",
    "@types/node": "24.10.1",
    "changelogen": "0.6.2",
    "eslint": "9.39.1",
    "jsdom": "^27.2.0",
    "nuxt": "4.2.1",
    "pkg-pr-new": "0.0.62",
    "playwright-core": "1.57.0",
    "typescript": "~5.9.3",
    "vitest": "4.0.14",
    "vue-tsc": "3.1.5"
  },
  "resolutions": {
    "@nuxt/a11y": "workspace:*"
  }
}
